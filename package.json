{
  "name": "EZ-Game-Audio-Conversion",
  "version": "1.4.3",
  "description": "Unattended batch conversion for game audio file types with loop tag support! Powered by ffmpeg.",
  "main": "app.js",
  "bin": {
    "ez-game-audio-conversion": "src/app.js"
  },
  "scripts": {
    "start": "node ./src/app.js",
    "build": "set PKG_ENV=packaging && pkg . -o ./dist/EZ-Game-Audio-Conversion/EZ-Game-Audio.exe --compress GZIP && node src/ico/icon.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pkg": {
    "scripts": [
      "./src/*.js"
    ],
    "targets": [
      "node18-win-x64"
    ]
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "fs": "^0.0.1-security",
    "moment": "^2.30.1",
    "path": "^0.12.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpaceFoon/Ez-Game-Audio-Conversion.git"
  },
  "keywords": [
    "Audio",
    "ffmpeg",
    "transcoding"
  ],
  "author": "Fooney",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/SpaceFoon/Ez-Game-Audio-Conversion/issues"
  },
  "homepage": "https://github.com/SpaceFoon/Ez-Game-Audio-Conversion#readme",
  "devDependencies": {
    "pkg": "^5.8.1",
    "resedit": "^2.0.0"
  }
}
