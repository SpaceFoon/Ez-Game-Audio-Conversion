{
  "name": "ez-game-audio-conversion",
  "version": "1.3",
  "description": "Unattended batch conversion of audio file types with recursive searching. Powered by ffmpeg.",
  "bin": "app.js",
  "pkg": {
    "scripts": [
      "./*.js",
      "./converterWorker.js"
    ],
    "assets": [
      "./ffmpeg.exe"
    ],
    "targets": [
      "node16-win-x86"
    ],
    "outputPath": "../dist"
  },
  "dependencies": {
    "agent-base": "^6.0.2",
    "async": "^3.2.5",
    "buffer-from": "^1.1.2",
    "caseless": "^0.12.0",
    "chalk": "^4.1.2",
    "concat-stream": "^2.0.0",
    "coverage": "^0.4.1",
    "debug": "^4.3.4",
    "env-paths": "^2.2.1",
    "ffmpeg": "^0.0.4",
    "ffmpeg-static": "^5.2.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs": "^0.0.1-security",
    "http-response-object": "^3.0.2",
    "https-proxy-agent": "^5.0.1",
    "inherits": "^2.0.4",
    "isexe": "^2.0.0",
    "jquery": "^2.2.4",
    "jquery-bridget": "^1.1.0",
    "moment": "^2.30.1",
    "ms": "^2.1.2",
    "nexe": "^4.0.0-rc.4",
    "parse-cache-control": "^1.0.1",
    "path": "^0.12.7",
    "pkg": "^5.8.1",
    "process": "^0.11.10",
    "progress": "^2.0.3",
    "readable-stream": "^3.6.2",
    "redline": "^1.0.1",
    "safe-buffer": "^5.2.1",
    "string_decoder": "^1.3.0",
    "typedarray": "^0.0.6",
    "util": "^0.10.4",
    "util-deprecate": "^1.0.2",
    "when": "^3.7.8",
    "which": "^1.3.1",
    "workerpool": "^9.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpaceFoon/Ez-Game-Audio-Conversion.git"
  },
  "keywords": [
    "Audio",
    "ffmpeg",
    "transcoding"
  ],
